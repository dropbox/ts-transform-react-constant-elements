{
  "name": "ts-transform-react-constant-elements",
  "version": "2.0.0",
  "description": "This plugin can speed up reconciliation and reduce garbage collection pressure by hoisting React elements to the highest possible scope, preventing multiple unnecessary reinstantiations.",
  "main": "dist/index.js",
  "scripts": {
    "test": "rm -rf test/fixture/*.js && tsc && mocha --require ts-node/register test/*.test.ts",
    "prettier": "prettier --print-width=120 --tab-width=4 --single-quote --trailing-comma=es5 --no-semi --parser=typescript --write 'src/**/*.ts*' || true",
    "prepublishOnly": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dropbox/ts-transform-react-constant-elements.git"
  },
  "keywords": [
    "typescript",
    "transform",
    "ts",
    "react",
    "constant",
    "elements"
  ],
  "author": "Long Ho <longho@dropbox.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/dropbox/ts-transform-react-constant-elements/issues"
  },
  "homepage": "https://github.com/dropbox/ts-transform-react-constant-elements#readme",
  "dependencies": {
    "typescript": "4"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/fs-extra": "^9.0.4",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^14.14.9",
    "chai": "^4.2.0",
    "fs-extra": "^9.0.1",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "pre-commit": "^1.2.2",
    "prettier": "^2.2.0",
    "react": "^18.2.0",
    "ts-node": "^9.0.0"
  }
}
